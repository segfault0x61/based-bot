    "bufferutil": "^4.0.6",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "octokit": "^1.7.1",
    "utf-8-validate": "^5.0.9",
    "winston": "^3.7.2"
