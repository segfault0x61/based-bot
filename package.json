{
  "name": "based-bot",
  "version": "1.0.0",
  "description": "Yet another general purpose Discord bot.",
  "main": "index.js",
  "scripts": {
    "start": "cd dist && node index.js",
    "dev": "ts-node ./src/index.ts",
